{
  "name": "stockholm",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel --filter \"./apps/*\" run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "typecheck": "pnpm -r --parallel typecheck",
    "prisma:generate": "pnpm -F @stockholm/db run generate",
    "prisma:migrate:dev": "pnpm -F @stockholm/db run migrate:dev",
    "prisma:migrate:deploy": "pnpm -F @stockholm/db run migrate:deploy",
    "prisma:studio": "pnpm -F @stockholm/db run studio"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.879.0",
    "@aws-sdk/s3-request-presigner": "^3.879.0",
    "@prisma/client": "^5.22.0",
    "@zxing/browser": "^0.1.5",
    "bcryptjs": "^3.0.2",
    "bwip-js": "^4.7.0",
    "ioredis": "^5.7.0",
    "papaparse": "^5.5.3",
    "pdf-lib": "^1.17.1",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.3",
    "xlsx": "^0.18.5",
    "zod": "^4.1.1"
  },
  "pnpm": {
    "overrides": {}
  }
}
